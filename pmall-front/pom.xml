    <groupId>com.sj</groupId>
    <artifactId>pmall-front</artifactId>
    <version>1.0-SNAPSHOT</version>
    <name>pmall-front</name>
    <description>Demo project for Spring Boot</description>

    <properties>
        <java.version>1.8</java.version>
